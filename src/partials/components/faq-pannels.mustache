<section class="b-faq py-5">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <h3>Still have questions</h3>
        <!-- Outer panel -->
        {{#outer-panels}}
        <div class="panel">
          <div class="panel__title" data-js="panel__title">
            {{outer-title}}
          </div>
          <div class="panel__body">
            <!-- Inner panels -->
            {{#inner-panels}}
            <div class="panel">
              <div class="panel__title" data-js="panel__title">
                {{inner-title}}
              </div>
              <div class="panel__body">
                <p>
                  {{inner-body}}
                </p>
              </div>
            </div>
            {{/inner-panels}}
            <!-- /Inner panels -->
          </div>
        </div>
        {{/outer-panels}}
        <!-- /Outer panel -->
      </div>
    </div>
  </div>
</section>